<p id="notice"><%= notice %></p>

<h1>Userdetails</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Dateofbirth</th>
      <th>Gender</th>
      <th>Country</th>
      <th>Interests</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @userdetails.each do |userdetail| %>
      <tr>
        <td><%= userdetail.firstname %></td>
        <td><%= userdetail.lastname %></td>
        <td><%= userdetail.dateofbirth %></td>
        <td><%= userdetail.gender %></td>
        <td><%= userdetail.country %></td>
        <td><%= userdetail.interests %></td>
        <td><%= link_to 'Show', userdetail %></td>
        <td><%= link_to 'Edit', edit_userdetail_path(userdetail) %></td>
        <td><%= link_to 'Destroy', userdetail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Userdetail', new_userdetail_path %>
